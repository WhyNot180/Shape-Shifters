config_version=4

_global_script_classes=[ {
"base": "ConvexPolygonShape2D",
"class": "ShapePoints",
"language": "GDScript",
"path": "res://ShapePoints.gd"
}, {
"base": "CollisionPolygon2D",
"class": "ShapePoints2",
"language": "GDScript",
"path": "res://ShapePoints2.gd"
}, {
"base": "Label",
"class": "Stopwatch",
